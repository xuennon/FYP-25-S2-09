wise_workout_app/
!wise_workout_app/build/web

# Git files
.git/
.gitignore

# Documentation
*.md
!README.md

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Log files
*.log
logs/

